<h1>Clientes</h1>


<table class="table table-striped table-responsive-md">
  <thead>
    <tr>
      <th>ID</th>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Cel</th>
      <th>Email</th>
    </tr>
  </thead>

  <tbody>
    <% @clients.each do |c|%>
      <tr>
        <td><%= c.id %></td>
        <td><%= c.first_name %></td>
        <td><%= c.last_name %></td>
        <td><%= c.cel %></td>
        <td><%= c.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
