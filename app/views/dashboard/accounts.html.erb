<% if current_account.admin? %>
  <h1>Cuentas</h1>


  <table class="table table-striped table-responsive-md">
    <thead>
      <tr>
        <th>ID</th>
        <th>Nombre</th>
        <th>Apellido</th>
        <th>Email</th>
        <th>Telefono</th>
        <th>Propiedades</th>
        <th>Prospectos</th>
        <th>Ãšltimo inicio</th>
      </tr>
    </thead>

    <tbody>
      <% @accounts.each do |account| %>
        <tr>
          <td><%= account.id %></td>
          <td><%= account.first_name %></td>
          <td><%= account.last_name %></td>
          <td><%= account.email %></td>
          <td><%= account.url %></td>
          <td><%=link_to "Ver", profile_path(account) %></td>
          <td><%=link_to "Ver", misprospectos_path(account) %></td>
          <td><%= account.last_sign_in_at.strftime("%a %d %b %y %l:%M %p")%></td>
        </tr>
      <% end %>
    </tbody>
  </table>
<%end%>
